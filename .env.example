# Local developer overrides
# Relax perf times for local development, default is 1 (strict)
PERF_TIME_MULTIPLIER=1
# Copy this file to .env and fill in values for production
# Do NOT commit the real .env to version control

# API & backend
API_URL=https://api.example.com
GRAPHQL_ENDPOINT=

# Third-party services
SENTRY_DSN=
GOOGLE_MAPS_API_KEY=
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=

# Analytics
ANALYTICS_KEY=

# Feature flags
FEATURE_FLAG_NEW_MAP=false

# Expo / EAS
EAS_BUILD_SECRET=

# Add any other environment-specific keys here

# OpenRouteService (ORS) - required for routing (copy to .env.local and fill)
# EXPO_ prefix is used so Expo/React Native will expose them to the JS runtime
EXPO_PUBLIC_ORS_API_KEY=
EXPO_PUBLIC_ORS_BASE_URL=https://api.openrouteservice.org
EXPO_PUBLIC_ORS_PROFILE=foot-walking
EXPO_PUBLIC_ORS_TIMEOUT=15000

# OTP2 (OpenTripPlanner) - optional; used for transit planning / GTFS
OTP2_BASE_URL=http://localhost:8080
OTP2_ROUTER_ID=default

# Map / Tiles (optional)
EXPO_PUBLIC_MAP_STYLE_URL=
EXPO_PUBLIC_MAPBOX_TOKEN=

# Local development guidance:
# 1) Copy this file to an untracked local file and edit values:
#      cp .env.example .env.local
# 2) Edit .env.local and set EXPO_PUBLIC_ORS_API_KEY to your ORS key.
# 3) The repository's .gitignore already ignores .env*.local, so .env.local will not be committed.

